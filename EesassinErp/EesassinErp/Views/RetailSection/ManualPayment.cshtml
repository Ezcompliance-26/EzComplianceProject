
@{
    ViewBag.Title = "Manual Payment";
    ViewBag.TitleList = "Sort By Column";
    ViewBag.Version = System.Configuration.ConfigurationManager.AppSettings["version"];
    Layout = "~/Views/Common/_IframeMaster.cshtml";
}
 
<script src="~/scripts/RetailJsLib/ManualBulkPaymetController.js"></script>
<input type="hidden" id="Id" name="Id" value="" ng-model="hfId" />
<div class="autohide-scroll autoheightset">
    <div class="br-pageheader">
        <!-- Title -->
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 root-t-p-n ">
            <h5 class="txt-dark">Manual Payment</h5>
        </div>
        <!-- Breadcrumb -->
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 hidden-sm">
            <ol class="breadcrumb">
                <li><span>Global</span></li>
                <li><span>Master</span></li>
                <li class="active"><span>Manual Payment</span></li>
            </ol>
        </div>
    </div>
    <div class="br-pagebody">
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="panel panel-default card-view shadow-base panel-refresh panel-Search-refresh">
                    <div class="refresh-container">
                        <div class="preloader">
                            <div class="circles-1">
                                <div class="circles-1-center"></div>
                                <div class="circles-1"></div>
                                <div class="circles-1"></div>
                                <div class="circles-1"></div>
                                <div class="circles-1"></div>
                                <div class="circles-1"></div>
                                <div class="circles-1"></div>
                            </div>
                        </div>
                    </div>
                    <div class="panel-wrapper ">
                        <div class="panel-body">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 no-padding ">
                                <div class="collapse in" id="collapseinputbox">
                                    <form id="user-form">
                                        <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12 mgbt-xs-15" >
                                            <label class="control-label">UnPaid Licence<span class="vd_red">*</span></label>
                                            <div class="input-box" ng-init="GetUnpaidLicence()">
                                                <select id="ddlModuleType" autocomplete='address-level2' class="select2 form-control validate" ng-model="LicenceId" ng-change="ChangeSetValue(LicenceId)">
                                                    <option value="">Select</option>
                                                    <option ng-repeat="x in UnpaidLicenceList" value="{{x.LicenceRequstId}}">{{x.ALicence}}</option>
                                                </select>
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Store Code<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                <input ng-model="StoreCode"   class="select2 form-control" disabled/> 
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">License Name<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                 <input ng-model="LicenseName" class="select2 form-control validate"   disabled/> 
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Invoice Amount<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                 <input ng-model="InvoiceAmount" class="select2 form-control validate"  disabled /> 
                                                 <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Payment Status</label>
                                            <div class="input-box">
                                                <input ng-model="PaymentStatus" class="select2 form-control validate"  disabled /> 
                                              
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4 col-md-3 col-sm-6 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Payment Over Due Date</label>
                                            <div class="input-box">
                                                <input ng-model="PaymentOverDueDate" class="select2 form-control validate" disabled  /> 
                                                 <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4 col-md-3 col-sm-6 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">License Type</label>
                                            <div class="input-box">
                                                <input ng-model="LicenseType" class="select2 form-control validate"  disabled />
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 ">
                                            @Html.Partial("ActionView")
                                        </div>

                                    </form>
                                </div>
                            </div>
                            @Html.Partial("SearchAngularJs")
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
