
@{
    ViewBag.Title = "Client Subscription";
    ViewBag.TitleList = "Sort By Column";
    ViewBag.Version = System.Configuration.ConfigurationManager.AppSettings["version"];
    Layout = "~/Views/Common/_IframeMaster.cshtml";
}

<script src="~/Scripts/myjslib/CientSubscriptController.js?Ver=@ViewBag.Version"></script>
<script type="text/javascript">

</script>
<input type="hidden" id="Id" name="Id" value="" ng-model="hfId" />
<div class="autohide-scroll autoheightset">
    <div class="br-pageheader">
        <!-- Title -->
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 root-t-p-n ">
            <h5 class="txt-dark">Client Subscription</h5>
        </div>
        <!-- Breadcrumb -->
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 hidden-sm">
            <ol class="breadcrumb">
                <li><span>Global</span></li>
                <li><span>Master</span></li>
                <li class="active"><span>Client Subscription</span></li>
            </ol>
        </div>

    </div>
    <div class="br-pagebody">
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="panel panel-default card-view shadow-base panel-refresh panel-Search-refresh">
                    <div class="refresh-container">
                        <div class="preloader">
                            <div class="circles-1">
                                <div class="circles-1-center"></div>
                                <div class="circles-1"></div>
                                <div class="circles-1"></div>
                                <div class="circles-1"></div>
                                <div class="circles-1"></div>
                                <div class="circles-1"></div>
                                <div class="circles-1"></div>
                            </div>
                        </div>
                    </div>
                    <div class="panel-wrapper ">
                        <div class="panel-body">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 no-padding ">
                                <div class="collapse in" id="collapseinputbox">
                                    <form id="user-form">

                                        <div class="col-md-4 col-sm-4 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">User Name<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                <input class="form-control clear validate" id="txtusername" ng-model="UName" type="text" placeholder="User Name" />
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 col-sm-4 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Address<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                @Html.TextBox("txtaddress", "", new { @class = "form-control clear validate", @placeholder = "", @ng_model = "Uaddress" })
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-2 col-sm-2 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Pincode<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                <input class="form-control clear validate" id="txtpincode" ng-model="PinCode" ng-change="AllPincode()" type="text" maxlength="6" placeholder="Pincode" />
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-2 col-sm-2 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Country<span class="vd_red">*</span></label>
                                            <div class="input-box" ng-init="AllCountry()">
                                                <select id="ddlcountry" autocomplete='address-level2' class="select2 form-control validate" ng-change="AllState()" ng-model="CountryId">
                                                    <option value="">Select Country</option>
                                                    <option ng-repeat="x in AllCountryList" value="{{x.CountryId}}">{{x.CountryName}}</option>
                                                </select>
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 col-sm-4 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">State<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                <select id="ddlstate" autocomplete='address-level2' class="select2 form-control" ng-change="AllCity()" ng-model="StateId">
                                                    <option value="">Select State</option>
                                                    <option ng-repeat="x in AllStateList" value="{{x.SATE_CODE}}">{{x.STATE_NM}}</option>
                                                </select>
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 col-sm-4 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">City<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                <select id="ddlcity" autocomplete='address-level2' class="select2 form-control" ng-model="CityId">
                                                    <option value="">Select City</option>
                                                    <option ng-repeat="x in AllCityList" value="{{x.CITY_CODE}}">{{x.CITY_NAME}}</option>
                                                </select>
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 col-sm-4 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">GST No<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                <input class="form-control clear validate" id="txtgstno" ng-model="Gstnno" type="text" placeholder="GST No" />
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 col-sm-4 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Registered company name<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                <input class="form-control clear validate" id="txtregistcomname" ng-model="RegComName" type="text" placeholder="Registered company name" />
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 col-sm-4 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Email ID<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                <input class="form-control clear validate" id="txtEmailID" ng-model="EmailId" type="email" placeholder="Email ID" />
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div> 
                                        <div class="col-md-4 col-sm-4 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Mobile no<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                <input class="form-control clear validate" id="txtMobile" ng-model="MobileNo" type="text" placeholder="Mobile no" />
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                          

                                        <div class="col-md-4 col-sm-6 col-xs-12 mgbt-xs-15" style="display:none;">
                                            <label class="control-label">Is Active<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                @Html.Partial("ddlActive")
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 ">
                                            @Html.Partial("ActionView")
                                        </div>

                                    </form>
                                </div>
                            </div>
                            @Html.Partial("SearchAngularJs")
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Row -->
    </div>

</div>

