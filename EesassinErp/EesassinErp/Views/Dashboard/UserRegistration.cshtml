

@{
    ViewBag.Title = "User Master";
    ViewBag.TitleList = "Sort By Column";
    ViewBag.Version = System.Configuration.ConfigurationManager.AppSettings["version"];
    Layout = "~/Views/Common/_IframeMaster.cshtml";
}
<script src="~/scripts/myjslib/UserRegistrationController.js"></script>

 

<div class="autohide-scroll autoheightset" ng-init="ResetControl()">
    <div class="br-pageheader">
        <!-- Title -->
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 root-t-p-n ">
            <h5 class="txt-dark">User Registration</h5>
        </div>
        <!-- Breadcrumb -->
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 hidden-sm">
            <ol class="breadcrumb">
                <li><span>Global</span></li>
                <li><span>Master</span></li>
                <li class="active"><span>User Registration</span></li>
            </ol>
        </div>

    </div>
    <div class="br-pagebody">
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="panel panel-default card-view shadow-base panel-refresh panel-Search-refresh">
                    <div class="refresh-container">
                        <div class="preloader">
                            <div class="circles-1">
                                <div class="circles-1-center"></div>
                                <div class="circles-1"></div>
                                <div class="circles-1"></div>
                                <div class="circles-1"></div>
                                <div class="circles-1"></div>
                                <div class="circles-1"></div>
                                <div class="circles-1"></div>
                            </div>
                        </div>
                    </div>
                    <div class="panel-wrapper ">
                        <div class="panel-body">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 no-padding ">
                                <div class="collapse in" id="collapseinputbox">
                                    <form id="user-form">

                                        <div class="col-md-4 col-sm-6 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Party Type<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                <select id="ddlVT" autocomplete='address-level2' class="select2 form-control validate" ng-model="PartyType" ng-change="AllParty()">
                                                    <option value="">Select </option>
                                                    <option value="Vendor">Vendor</option>
                                                    <option value="Auditor">Auditor</option>
                                                    <option value="Client">Client</option>
                                                    @*<option value="Other">Other</option>*@
                                                </select>
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 col-sm-6 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Party Name<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                <select id="ddlPartyId" autocomplete='address-level2' class="select2 form-control validate" ng-change="SetName()" ng-model="PartyId">
                                                    <option value="">Select Party</option>
                                                    <option ng-repeat="x in AllPartyList" value="{{x.PartyId}}">{{x.PartyName}}</option>
                                                </select>
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 col-sm-6 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Name<span class="vd_red">*</span></label>
                                            <div class="input-box">

                                                <input id="txtName" class="form-control clear validate" ng-model="EmpName" ng-keyup="SetUserNamePassword();" />
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-4 col-sm-6 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">User Name<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                @*@Html.TextBox("txtUserName", "", new { @class = "form-control clear validate", @placeholder = "", @ng_model = "UserName", ng_keyup = "SetUserNamePassword();" })*@
                                            <input id="txtUser" class= "form-control clear validate" placeholder = "" ng-model = "UserNames"   />
                                                
                                                    <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-4 col-sm-6 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Password<span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                @Html.TextBox("txtPass", "", new { @class = "form-control clear validate", @placeholder = "", @ng_model = "Password", @type = "password"})
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 col-sm-6 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Contact No <span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                <input id="txtContactNo" class="form-control clear validate" maxlength = "10" placeholder="" ng-model="ContactNumber" onblur="validatePhoneNo('#txtContactNo')"  onKeyUp="$(this).val($(this).val().replace(/[^\d]/ig, ''))"  />
                                                @*@Html.TextBox("txtContactNo", "", new { @class = "form-control clear validate", @onblur = "validatePhoneNo('#txtContactNo')", @maxlength = "10", @placeholder = "", @ng_model = "ContactNumber" })*@
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 col-sm-6 col-xs-12 mgbt-xs-15">
                                            <label class="control-label">Email Id <span class="vd_red">*</span></label>
                                            <div class="input-box">
                                                <input id="txtEmail" class="form-control clear validate" ng-model="EmailId" type="email" onblur="validateEmail('#txtEmail')" />
                                                <div class="text-box-msg-r">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 ">
                                            @Html.Partial("ActionView")
                                        </div>

                                    </form>
                                </div>
                            </div>
                            @Html.Partial("SearchAngularJs")
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Row -->
    </div>

</div>