
<div id="printDiv" ng-init="GetReportHeaderUsingCategory('General Report');">
    <table class="bill-box bill-box2 page-ba">
        <tbody>
            <tr ng-repeat="id in InvoicesData">
                <td>
                    <div class="bill-box-top">
                        <div id="ReportHeader" ng-bind-html="ReportHeader"></div>
                        <div style="width: 100%;  ">
                            <div class="invoice-t">
                                <div>
                                    Conversion Date. <span ng-bind="id.ConvertionDate"></span>
                                </div>
                            </div>
                            <div class="invoice-t invoice text-center">
                                <span ng-bind="id.I_Title"></span><br />
                                <span class="small-t">({{id.SubTitle}} Customer Copy)</span>
                            </div>
                            <div class="invoice-t text-right">Conversion No. <span ng-bind="id.ConvertionNo"></span> </div>

                        </div>




                        <div class="details-receiver">
                            <div class="col-sm-2">
                                Details of Farmer
                            </div>
                            <div class="col-sm-2">
                                Details of Cunsumed Stock 
                            </div>
                        </div>

                        <div class="transportation2">
                            <div class="col-sm-2">
                                <table>
                                    <tr>
                                        <td style="width: 97px;">Farmer Name</td>
                                        <td ng-bind="id.FarmerName"></td>
                                    </tr>
                                    <tr>
                                        <td style="width: 97px;">Farm Code</td>
                                        <td ng-bind="id.FarmCode"></td>
                                    </tr>
                                    <tr>
                                        <td style="width: 97px;">Address</td>
                                        <td ng-bind="id.Address"></td>
                                    </tr>
                                    <tr>
                                        <td style="width: 97px;">Contact No</td>
                                        <td ng-bind="id.ContactNo"></td>
                                    </tr>

                                    <tr>
                                        <td style="width: 120px;">SuperVisionDay</td>
                                        <td ng-bind="id.SuperVisionDay"></td>
                                    </tr>
                                   

                                </table>


                            </div>
                            <div class="col-sm-2" style="float:right; border-right:none;">

                                <table>

                                    <tr>
                                        <td style="width: 97px;">Feed Cunsumed</td>
                                        <td ng-bind="id.FeedCunsumed"></td>
                                    </tr>
                                    <tr>
                                        <td style="width: 99px;">Medicine Cunsumed</td>
                                        <td ng-bind="id.MedicineCunsumed"></td>
                                    </tr>
                                    <tr>
                                        <td style="width: 97px;">Total Mortality</td>
                                        <td ng-bind="id.TotalMortality"></td>
                                    </tr>
                                    <tr>
                                        <td style="width: 100%;">Total Bird Prepared</td>
                                        <td ng-bind="id.BirdPrepared"></td>
                                    </tr>

                                </table>
                            </div>
                        </div>
                    </div>

                    <div class="bill-box-table">
                        <table>
                            <thead>
                                <tr>
                                    <th style="width:47px">Sr. No.</th>
                                    <th>Allotment Date</th>
                                    <th>Item Type</th>
                                    <th>Item</th>
                                    <th style="white-space: nowrap;">Unit</th>
                                    <th style="white-space: nowrap;">Qty.(In Bags)</th>
                                    <th style="white-space: nowrap;">Qty.Per Bag</th>
                                    <th style="white-space: nowrap;">Total Quantity</th>
                                    <th style="width:100px;vertical-align:middle; text-align:center;">Rate</th>
                                    <th style="width:100px;vertical-align:middle; text-align:center;">Amount</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="DD in DetailData">
                                    {{DD.SNO}}
                                    <td style="width:40px" ng-if="DD.SNO!=99">{{DD.SNO}}</td>

                                    <td style="width:40px" ng-if="DD.SNO==99"></td>
                                    <td>{{DD.AllotmentDate}}</td>
                                    <td>{{DD.SupplierType}}</td>
                                    <td>{{DD.Item}}</td>
                                    <td style="text-align:right;">{{DD.Unit}}</td>
                                    <td style="text-align:right;">{{DD.QtyInBag}}</td>
                                    <td style="text-align:right;">{{DD.QtyPerBag}}</td>
                                    <td style="text-align:right;">{{DD.TotalQuantity}}</td>
                                    <td style="text-align:right;">{{ConvertToTwoDecimalPoint(DD.Rate)}}</td>
                                    <td style="text-align:right;">{{ConvertToTwoDecimalPoint(DD.Amount)}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>

                    

                    <div class="bill-box-top">
                        <div class="transportation2" style="border-bottom: solid 1px;">
                            <div class="col-sm-2">
                                <div class="invoice-value">
                                    <span>Invoice Value (In Words)</span> <span ng-bind="InWords"></span>
                                </div>


                            </div>
                            <div class="col-sm-2" style="float:right; border-left: solid 1px;">

                                <table>

                                    <tr>
                                        <td style="width: 100%;">Total Cost</td>
                                        <td ng-bind="TotalCost"></td>
                                    </tr>
                                    <tr>
                                        <td style="width: 100%">One Hen Weight Should be</td>
                                        <td ng-bind="OnehenWeight"></td>
                                    </tr>
                                    <tr>
                                        <td style="width: 100%">Rate Per Hen</td>
                                        <td ng-bind="RatePerHen"></td>
                                    </tr>
                                    <tr>
                                        <td style="width: 100%;">Excepted Production</td>
                                        <td ng-bind="ExceptedProducation"></td>
                                    </tr>
                                    <tr>
                                        <td style="width: 100%;">Total Production</td>
                                        <td ng-bind="TotalProducation"></td>
                                    </tr>
                                    <tr>
                                        <td style="width: 100%;">Excess/Storage</td>
                                        <td ng-bind="ExcessShortage"></td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                        
                        <div class="terms-conditions">

                            <table class="TC-table">
                                <tbody>
                                    <tr>
                                        <td style="width:60%;vertical-align:bottom;">

                                            <div class="terms-left">
                                                <span>Narration</span>
                                                {{Remarks}}
                                            </div>
                                            <div class="terms-left">
                                                <span>{{Term}}</span>
                                            </div>
                                        </td>
                                        <td>
                                            <div class="terms-right">
                                                <div class="for-company">
                                                    For {{BranchName}}
                                                </div>
                                                <div class="authorised">
                                                    Authorised Signatory
                                                </div>
                                            </div>
                                        </td>
                                    </tr>

                                </tbody>
                            </table>

                        </div>

                    </div>
                </td>
            </tr>
        </tbody>
    </table>
