
<div id="printDiv" ng-init="GetReportHeaderUsingCategory('General Report');">
    <table class="bill-box bill-box2 page-ba">
        <tbody>
            <tr ng-repeat="id in InvoicesData">
                <td>
                    <div class="bill-box-top">
                        <div id="ReportHeader" ng-bind-html="ReportHeader"></div>
                        <div style="width: 100%;  ">
                            <div class="invoice-t">
                                <div>
                                    Conversion Date. <span ng-bind="id.ConvertionDate"></span>
                                </div>
                            </div>
                            <div class="invoice-t invoice text-center">
                                <span ng-bind="id.I_Title"></span><br />
                                <span class="small-t">({{id.SubTitle}} Customer Copy)</span>
                            </div>
                            <div class="invoice-t text-right">Conversion No. <span ng-bind="id.ConvertionNo"></span> </div>

                        </div>




                        <div class="details-receiver">
                            <div class="col-sm-2">
                                Details of Farmer
                            </div>
                          
                        </div>

                        <div class="transportation2">
                            <div class="col-sm-2">
                                <table>
                                    <tr>
                                        <td style="width: 97px;">Farmer Name</td>
                                        <td ng-bind="id.FarmerName"></td>
                                    </tr>
                                    <tr>
                                        <td style="width: 97px;">Farm Code</td>
                                        <td ng-bind="id.FarmCode"></td>
                                    </tr>
                                    <tr>
                                        <td style="width: 97px;">Address</td>
                                        <td ng-bind="id.Address"></td>
                                    </tr>
                                    <tr>
                                        <td style="width: 97px;">Contact No</td>
                                        <td ng-bind="id.ContactNo"></td>
                                    </tr>

                                    <tr>
                                        <td style="width: 120px;">Pin code</td>
                                        <td ng-bind="id.Pincode"></td>
                                    </tr>


                                </table>


                            </div>
                            <div class="col-sm-2" style="float:right; border-right:none;">

                                <table>

                                    <tr>
                                        <td style="width: 97px;">GSTIN UIN </td>
                                        <td ng-bind="id.GSTINUIN"></td>
                                    </tr>
                                    <tr>
                                        <td style="width: 99px;">PANITNO</td>
                                        <td ng-bind="id.PANITNO"></td>
                                    </tr>
                                    
                                </table>
                            </div>
                        </div>
                    </div>

                    <div class="bill-box-table">
                        <table>
                            <thead>
                                <tr>
                                    <th class="text-right" colspan="3"></th>
                                    <th style="text-align:center" colspan="3">Mortality</th>
                                    <th class="text-right" colspan="2">Feed Intake</th>
                                    <th class="text-right" colspan="2">Cum Feed</th>
                                    <th class="text-right" colspan="2">Body Wt(gm)</th>
                                    <th class="text-right" colspan="2">FCR</th>
                                </tr>
                                <tr>
                                    <th style="width:47px">Sr. No.</th> 
                                    <th>SuperVision Date</th>
                                    <th>Age</th>
                                    <th style="white-space: nowrap;">Daily</th>
                                    <th style="white-space: nowrap;">Total</th>
                                    <th style="white-space: nowrap;">Cum</th> 
                                    <th style="white-space: nowrap;">Std</th>
                                    <th style="white-space: nowrap;">Actual</th>
                                    <th style="white-space: nowrap;">Feed</th>
                                    <th style="white-space: nowrap;">Actual</th>
                                    <th style="white-space: nowrap;">Min</th>
                                    <th style="white-space: nowrap;">Actual</th>
                                    <th style="white-space: nowrap;">Std</th>
                                    <th style="white-space: nowrap;">Actual</th> 
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="DD in DetailData">
                                    {{DD.SNO}}
                                    <td style="width:40px" ng-if="DD.SNO!=99">{{DD.SNO}}</td>

                                    <td style="width:40px" ng-if="DD.SNO==99"></td>
                                  
                                    <td>{{DD.SupervisionDate}}</td>
                                    <td>{{DD.Age}}</td>
                                    <td style="text-align:right;">{{DD.MortailtyDaily}}</td>
                                    <td style="text-align:right;">{{DD.MortailtyTotal}}</td>
                                    <td style="text-align:right;">{{DD.MortailtyCum}}</td>
                                    <td style="text-align:right;">{{DD.FeedStd}}</td>
                                    <td style="text-align:right;">{{DD.FeedActual}}</td>
                                    <td style="text-align:right;">{{DD.CumFeed}}</td>
                                    <td style="text-align:right;">{{DD.CumActual}}</td>
                                    <td style="text-align:right;">{{DD.BodyMin}}</td>
                                    <td style="text-align:right;">{{DD.BodyActual}}</td>
                                    <td style="text-align:right;">{{DD.FCRStd}}</td>
                                    <td style="text-align:right;">{{DD.FCRActual}}</td>
                                  
                                </tr>
                            </tbody>
                        </table>
                    </div>



                     
                </td>
            </tr>
        </tbody>
    </table>
     
</div>







